      },
      {
        "name": "WORDPRESS_DB_PASSWORD",
        "label": "Database password"
      },
      {
        "name": "VIRTUAL_HOST",
        "label": "Virtual host name"
      }
    ],
    "ports": [
      "80/tcp"
    ],
    "volumes": ["/var/www/html"]
  }
]
